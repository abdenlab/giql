      "Transpiled SQL:\n",
      "WITH distances AS (SELECT a.chromosome, a.start_pos AS a_start, a.end_pos AS a_end, b.start_pos AS b_start, b.end_pos AS b_end, \n",
      "            CASE \n",
      "                WHEN a.\"chromosome\" != b.\"chromosome\" THEN NULL \n",
      "                WHEN a.\"start_pos\" < b.\"end_pos\" AND a.\"end_pos\" > b.\"start_pos\" THEN 0 \n",
      "                WHEN a.\"end_pos\" <= b.\"start_pos\" THEN (b.\"start_pos\" - a.\"end_pos\") \n",
      "                ELSE (a.\"start_pos\" - b.\"end_pos\") \n",
      "            END\n",
      "             AS signed_distance, ROW_NUMBER() OVER (PARTITION BY a.chromosome, a.start_pos, a.end_pos ORDER BY ABS(\n",
      "            CASE \n",
      "                WHEN a.\"chromosome\" != b.\"chromosome\" THEN NULL \n",
      "                WHEN a.\"start_pos\" < b.\"end_pos\" AND a.\"end_pos\" > b.\"start_pos\" THEN 0 \n",
      "                WHEN a.\"end_pos\" <= b.\"start_pos\" THEN (b.\"start_pos\" - a.\"end_pos\") \n",
      "                ELSE (a.\"start_pos\" - b.\"end_pos\") \n",
      "            END\n",
      "            )) AS rank FROM features_a AS a CROSS JOIN features_b AS b WHERE a.chromosome = b.chromosome AND a.chromosome = 'chr1' AND a.start_pos < 500000) SELECT chromosome, a_start, a_end, b_start, b_end, signed_distance, CASE WHEN signed_distance < 0 THEN 'upstream' WHEN signed_distance > 0 THEN 'downstream' ELSE 'overlap' END AS direction FROM distances WHERE rank = 1 ORDER BY chromosome, a_start\n",
