   "source": [
    "# Create synthetic data with strand information to demonstrate stranded distance\n",
    "import pandas as pd\n",
    "\n",
    "# Create test features with defined strands\n",
    "stranded_features_a = pd.DataFrame({\n",
    "    'chromosome': ['chr1', 'chr1', 'chr1', 'chr1'],\n",
    "    'start_pos': [1000, 2000, 3000, 4000],\n",
    "    'end_pos': [1100, 2100, 3100, 4100],\n",
    "    'strand': ['+', '+', '-', '-']\n",
    "})\n",
    "\n",
    "stranded_features_b = pd.DataFrame({\n",
    "    'chromosome': ['chr1', 'chr1', 'chr1', 'chr1'],\n",
    "    'start_pos': [1200, 2200, 3200, 4200],\n",
    "    'end_pos': [1300, 2300, 3300, 4300],\n",
    "    'strand': ['+', '-', '-', '+']\n",
    "})\n",
    "\n",
    "# Register with DuckDB\n",
    "engine.conn.register(\"stranded_a\", stranded_features_a)\n",
    "engine.conn.register(\"stranded_b\", stranded_features_b)\n",
    "\n",
    "# Register schemas\n",
    "for table in [\"stranded_a\", \"stranded_b\"]:\n",
    "    engine.register_table_schema(\n",
    "        table,\n",
    "        {\n",
    "            \"chromosome\": \"VARCHAR\",\n",
    "            \"start_pos\": \"BIGINT\",\n",
    "            \"end_pos\": \"BIGINT\",\n",
    "            \"strand\": \"VARCHAR\"\n",
    "        },\n",
    "        genomic_column=\"position\"\n",
    "    )\n",
    "\n",
    "# Query with stranded=true\n",
    "giql_query = \"\"\"\n",
    "    SELECT\n",
    "        a.chromosome,\n",
    "        a.start_pos AS a_start,\n",
    "        a.end_pos AS a_end,\n",
    "        a.strand AS a_strand,\n",
    "        b.start_pos AS b_start,\n",
    "        b.end_pos AS b_end,\n",
    "        b.strand AS b_strand,\n",
    "        DISTANCE(a.position, b.position, stranded=true) AS stranded_distance,\n",
    "        DISTANCE(a.position, b.position) AS unstranded_distance\n",
    "    FROM stranded_a a\n",
    "    CROSS JOIN stranded_b b\n",
    "    WHERE a.chromosome = b.chromosome\n",
    "    ORDER BY a.start_pos, b.start_pos\n",
    "\"\"\"\n",
    "\n",
    "sql = engine.transpile(giql_query)\n",
    "cursor = engine.conn.execute(sql)\n",
    "result = cursor.df()\n",
    "\n",
    "print(\"Stranded Distance Examples:\")\n",
    "print(\"\\nKey observations:\")\n",
    "print(f\"- Same '+' strands (row 0): distance = {result.iloc[0]['stranded_distance']} (positive)\")\n",
    "print(f\"- Different strands, '+' first (row 1): distance = {result.iloc[1]['stranded_distance']} (positive, sign based on first interval)\")\n",
    "print(f\"- Same '-' strands (row 10): distance = {result.iloc[10]['stranded_distance']} (negative!)\")\n",
    "print(f\"- Different strands, '-' first (row 11): distance = {result.iloc[11]['stranded_distance']} (negative, sign based on first interval)\")\n",
    "print(\"\\nFull results:\")\n",
    "result"
   ]
